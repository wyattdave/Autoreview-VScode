{
  "name": "autoreview-powerautomate",
  "displayName": "AutoReview for Power Automate",
  "description": "Extracts key information from Power Automate and Logic App flows",
  "version": "1.0.6",
  "icon": "assets/images/icon.png",
  "engines": {
    "vscode": "^1.70.2"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [   
  ],
  "repository": "https://github.com/wyattdave/Autoreview-VScode",
  "publisher":"PowerDevBox",
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "Autoreview.details",
        "title": "Flow Details"
      },
      {
        "command": "Autoreview.getJson",
        "title": "Flow Review"
      },
      {
        "command": "Autoreview.getConnections",
        "title": "Flow Connections"
      },
      {
        "command": "Autoreview.getActions",
        "title": "Flow Actions"
      },
      {
        "command": "Autoreview.getDiagram",
        "title": "Flow Diagram"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.70.2",
    "@types/mocha": "^10.0.8",
    "@types/node": "18.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}
